<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite parallel="none" name="SauceLab - Web Automation UI" thread-count="4">
	
	<listeners>
		<listener class-name="commons.MethodListener" />
		<listener class-name="reportConfig.AllureTestListener" />
	</listeners>


	<parameter name="url" value="https://www.saucedemo.com/" />
	
	<test name="Run on Firefox UI" preserve-order="true">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.saucelab.sort.Level_31_Sort_ASC_DESC" />
		</classes>
	</test>
	




</suite>
